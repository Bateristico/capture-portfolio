e';\r\n//Animations\r\nimport { motion } from 'framer-motion';\r\nimport { pageAnimation } from '../animation';\r\n\r\nconst MovieDetail = () => {\r\n    const history = useHistory();\r\n    const url = history.location.pathname;\r\n    const [ movies, setMovies ] = useState(MovieState);\r\n    const [ movie, setMovie ] = useState(null);\r\n\r\n    //useEffect\r\n    useEffect(() => {\r\n        const currentMovie = movies.filter((stateMovie) => stateMovie.url === url);\r\n        setMovie(currentMovie[0]);\r\n    }, [movies, url]);\r\n\r\n\r\n    return (\r\n        <>\r\n        {movie && (\r\n            <Details exit=\"exit\" variants={pageAnimation} initial=\"hidden\" animate=\"show\">\r\n                <HeadLine>\r\n                    <h2>{movie.title}</h2>\r\n                    <img src={movie.mainImg} alt=\"movie\"/>\r\n                </HeadLine>\r\n                <Awards>\r\n                    {movie.awards.map((award) => (\r\n                        <Award title={award.title} description={award.description} key={award.title}/>\r\n                    ))}\r\n                </Awards>\r\n                <ImageDisplay>\r\n                    <img src={movie.secondaryImg} alt=\"movie\"/>\r\n                </ImageDisplay>\r\n            </Details>\r\n        )}\r\n        </>\r\n    );\r\n};\r\n\r\nconst Details = styled(motion.div)`\r\n    color:white;\r\n\r\n`;\r\n\r\nconst HeadLine = styled.div`\r\n    min-height: 90vh;\r\n    padding-top: 20vh;\r\n    position: relative;\r\n    h2 {\r\n        position: absolute;\r\n        top: 10%;\r\n        left: 50%;\r\n        transform: translate(-50%, -10%)\r\n    }\r\n\r\n    img {\r\n        width: 100%;\r\n        height: 70vh;\r\n        object-fit: cover;\r\n    }\r\n`;\r\n\r\nconst Awards = styled.div`\r\n    min-height: 80vh;\r\n    display: flex;\r\n    margin: 5rem 10rem;\r\n    align-items: center;\r\n    justify-content: space-around;\r\n`;\r\n\r\nconst AwardStyle = styled.div`\r\n    padding: 5rem;\r\n    h3 {\r\n        font-size: 2rem;\r\n    }\r\n    .line {\r\n        width: 100%;\r\n        background: #23d997;\r\n        height: 0.5rem;\r\n        margin: 1rem 0rem;\r\n    }\r\n\r\n    p {\r\n        padding: 2rem 0rem;\r\n    }\r\n`;\r\n\r\nconst ImageDisplay = styled.div`\r\n    min-height: 50vh;\r\n    img {\r\n        width: 100%;\r\n        height: 100vh;\r\n        object-fit: cover;\r\n    }\r\n`;\r\n\r\n//Award component\r\nconst Award = ({title, description}) => {\r\n    return (\r\n        <AwardStyle>\r\n            <h3>{title}</h3>\r\n            <div className=\"line\"></div>\r\n            <p>{description}</p>\r\n        </AwardStyle>\r\n    );\r\n};\r\n\r\n\r\nexport default MovieDetail;","C:\\Users\\CamiloSaa\\Downloads\\GGS\\Bootcamps\\dev-ed-react-bootcamp\\react-portfolio-site\\capture\\src\\movieState.js",[],"C:\\Users\\CamiloSaa\\Downloads\\GGS\\Bootcamps\\dev-ed-react-bootcamp\\react-portfolio-site\\capture\\src\\animation.js",[],"C:\\Users\\CamiloSaa\\Downloads\\GGS\\Bootcamps\\dev-ed-react-bootcamp\\react-portfolio-site\\capture\\src\\components\\Wave.js",[],"C:\\Users\\CamiloSaa\\Downloads\\GGS\\Bootcamps\\dev-ed-react-bootcamp\\react-portfolio-site\\capture\\src\\components\\Toggle.js",[],"C:\\Users\\CamiloSaa\\Downloads\\GGS\\Bootcamps\\dev-ed-react-bootcamp\\react-portfolio-site\\capture\\src\\components\\useScroll.js",[],{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":12,"column":21,"nodeType":"85","messageId":"86","endLine":12,"endColumn":30},"no-native-reassign",["87"],"no-negated-in-lhs",["88"],"no-unused-vars","'setMovies' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]